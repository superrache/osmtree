{
  "name": "osmtree",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "lint": "eslint .",
    "dev:client": "vite",
    "dev:server": "nodemon",
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "build:client": "tsc && vite build",
    "build:server": "tsc -p tsconfig.server.json",
